{
  "name": "sherlihy_dot_com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "image": "packer init ./deployment/imaging/aws-ubuntu.pkr.hcl && packer build ./deployment/imaging/aws-ubuntu.pkr.hcl",
    "init-terraform": "cd deployment/production && terraform init -input=false && cd ../../",
    "deploy": "cd deployment/production && terraform destroy -auto-approve && terraform apply -auto-approve && cd ../../"
  },
  "dependencies": {
    "terraform": "^1.23.0",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
