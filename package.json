{
  "name": "sherlihy_dot_com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "diagram": "live-server --open=./current-diagrams",
    "preview": "vite preview",
    "image": "cd ./deployment/imaging && packer init aws-ubuntu.pkr.hcl && packer build aws-ubuntu.pkr.hcl && cd ../../",
    "deploy-prod": "cd ./deployment/production && ./plan_prod.sh && ./deploy_prod.sh && cd ../../",
    "setup-demo-deploy": "cd ./deployment/imaging/demo_instance && ./setup_demo_deploy.sh && cd ../../../",
    "deploy-demo-image": "cd ./deployment/imaging/demo_instance && ./deploy_demo_image.sh && cd ../../../",
        "dist_paths": "find ./dist -type f > ./deployment/s3_deploy/dist_file_paths.txt && awk -f ./deployment/s3_deploy/dist_paths_to_list.awk ./deployment/s3_deploy/dist_file_paths.txt > ./deployment/s3_deploy/s3_vars.tfvars",
        "stage_s3": "cd ./deployment/s3_deploy && terraform apply -var-file=\"./s3_vars.tfvars\"",
        "unstage_s3": "cd ./deployment/s3_deploy && terraform destroy -var-file=\"./s3_vars.tfvars\""
  },
  "dependencies": {
    "live-server": "^1.2.2",
    "mermaid": "^10.6.1",
    "terraform": "^1.23.0",
    "vue": "^3.3.8",
    "zero-md": "^2.5.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "marked": "^11.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
